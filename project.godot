config_version=5

[application]

config/name="ss-godot-game-boilerplate"
config/description="Boilerplate code for developing godot games"
run/main_scene="res://modes/main/main.tscn"
config/features=PackedStringArray("4.3")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://assets/images/icon.png"

[audio]

buses/default_bus_layout="res://assets/base/default_bus_layout.tres"

[autoload]

Constants="*res://global/Constants.gd"
Helpers="*res://global/Helpers.gd"
Utils="*res://global/Utils.gd"
ProfileManager="*res://global/ProfileManager.gd"
ConfigManager="*res://global/ConfigManager.gd"
AudioManager="*res://global/AudioManager.gd"
VideoManager="*res://global/VideoManager.gd"
InputManager="*res://global/InputManager.gd"
GameManager="*res://global/GameManager.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/viewport_width=854
window/size/viewport_height=480
window/size/resizable=false

[editor]

naming/node_name_num_separator=2
naming/node_name_casing=2

[gui]

theme/custom="res://assets/base/default_theme.tres"

